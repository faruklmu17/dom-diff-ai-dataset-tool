{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DomDiffMetadata",
    "type": "object",
    "required": [
      "sample_id",
      "page_type",
      "source",
      "created_at_utc"
    ],
    "properties": {
      "sample_id": {
        "type": "string",
        "description": "Unique ID for the sample, e.g. sample_001"
      },
      "page_type": {
        "type": "string",
        "description": "Type of page, e.g. login, dashboard, form"
      },
      "source": {
        "type": "string",
        "description": "Origin of this sample, e.g. custom_demo_app"
      },
      "created_by": {
        "type": "string",
        "description": "Creator of the dataset entry"
      },
      "created_at_utc": {
        "type": "string",
        "description": "Creation timestamp in UTC"
      },
      "dom_version_before": {
        "type": "string",
        "description": "Version label of the 'before' DOM"
      },
      "dom_version_after": {
        "type": "string",
        "description": "Version label of the 'after' DOM"
      },
      "change_summary": {
        "type": "object",
        "required": [
          "text_changes",
          "style_changes",
          "layout_changes",
          "elements_added",
          "elements_removed",
          "attribute_changes"
        ],
        "properties": {
          "text_changes": { "type": "integer" },
          "style_changes": { "type": "integer" },
          "layout_changes": { "type": "integer" },
          "elements_added": { "type": "integer" },
          "elements_removed": { "type": "integer" },
          "attribute_changes": { "type": "integer" }
        },
        "additionalProperties": false
      }
    },
    "additionalProperties": false
  }
  